<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
			fx:controller="viewFXML.FahradOverviewController">
   <children>
      <TableView fx:id="fahrradTablle" prefWidth="750.0">
        <columns>
          <TableColumn prefWidth="300.0" text="Bezeichnung">
               <columns>
                  <TableColumn fx:id="name" prefWidth="150.0" text="Name"/>
                  <TableColumn fx:id="artikelnummer" prefWidth="150.0" text="Artikelnummer" onEditCommit="#aenderArtikelnummer"/>
               </columns>
            </TableColumn>
          <TableColumn fx:id="typ" prefWidth="150.0" text="Typ" />
            <TableColumn fx:id="farbe" prefWidth="150.0" text="Farbe" />
            <TableColumn fx:id="zoll" prefWidth="150.0" text="Zoll" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
